<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.project.com.hygieia">


    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="app.project.com.hygieia.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="app.project.com.hygieia.SignupActivity" />
        <activity android:name="app.project.com.hygieia.MainScreen" />
        <activity
            android:name="app.project.com.hygieia.FrontScreen"
            android:label="@string/title_activity_front_screen"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>